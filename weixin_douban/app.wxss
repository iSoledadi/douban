/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
image{
  border-radius: 5px;
}
/* 通用功能二：灰色分割块 */
.divider{
  height:30rpx;
  background-color: #f1f1f1;
}
/* 通用功能：向右箭头 */
.arrow{
  width: 6px;
  height: 6px;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  transform: rotate(45deg);
}
